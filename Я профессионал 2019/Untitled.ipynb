    "import math\n",
    "def f(u, v):\n",
    "    return 1000 * u + 2000/u**0.5 * (math.e/v + math.log(v))\n",
    "\n",
    "ans = 100500\n",
    "for j in range(1, 16):\n",
    "    for k in range(2, 4):\n",
    "        ans = min(ans, f(j, k))\n",
    "print(ans)"
   ]
